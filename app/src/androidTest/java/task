Get Orders statistics

---Get order with date:
select count(product_id),product_name,o.order_date from order_items oi left join orders o on o.order_id=oi.order_id group by product_id






SELECT oi.order_id,oi.product_name,oi.product_code,oi.quantity,oi.price,oi.product_id,oi.category, count(product_id) as product_count,o.order_date as item_date from order_items oi left join orders o on o.order_id=oi.order_id WHERE item_date BETWEEN datetime('now', '-1 days') AND datetime('now', 'localtime') group by oi.product_id order by product_count desc



SELECT oi.order_id,oi.product_name,oi.product_code,oi.quantity,oi.price,oi.product_id,oi.category, count(product_id) as product_count,o.order_date as item_date from order_items oi left join orders o on o.order_id=oi.order_id WHERE item_date >= >= datetime('now','-1 hours') group by oi.product_id order by product_count desc


SELECT oi.order_id,oi.product_name,oi.product_code,oi.quantity,oi.price,oi.product_id,oi.category, count(product_id) as product_count,o.order_date as item_date from order_items oi left join orders o on o.order_id=oi.order_id WHERE item_date BETWEEN datetime('now', '-180 days') AND datetime('now', 'localtime') group by oi.product_id order by product_count desc

  ////////////////////////////////////////////////////
  SELECT oi.order_id,oi.product_name,oi.product_code,oi.quantity,oi.price,oi.product_id,oi.category, count(product_id) as product_count,o.order_date as item_date,count(oi.product_id) from order_items oi left join orders o on o.order_id=oi.order_id WHERE strftime('%m', item_date) = '11' group by oi.product_id order by product_count desc


  SELECT * FROM workoutPlan WHERE strftime('%m', date) = '11'

select c.store_id,c.store_name,c.store_code,c.phone,o.id,o.order_id,o.customer_id,oi.order_id,oi.product_id,count(oi.product_id) from customers c left join order_items oi on oi.order_id=o.id left join orders o on o.customer_id=c.store_id where oi.product_id=338 group by o.customer_id


