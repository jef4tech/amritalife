   "SELECT *,(select count(id) from call_comment where call_comment.customer_id = calls.customer_id) as count_comment FROM "
    + DATABASE_TABLE + " rc JOIN " + CustomersDB.DATABASE_TABLE + " c " + " ON rc.customer_id = c.store_id LEFT JOIN calls ON
    rc.route_customer_id = calls.route_customer_id AND rc.customer_id = calls.customer_id WHERE rc.route_customer_id = ?
    ORDER BY sort_order ASC"


(select count(id) from call_comment where call_comment.customer_id = calls.customer_id)


select product_name,order_date from order_items o left join orders on orders.order_id=o.order_id


SELECT *,(select count(id) from call_comment where call_comment.customer_id = calls.customer_id) as count_comment  FROM route_customers rc JOIN customers c ON rc.customer_id = c.store_id LEFT JOIN calls ON rc.route_customer_id = calls.route_customer_id where calls.status = "visited"   ORDER BY sort_order ASC


