KEY_SAMPLES_GIVEN--NextVisitedDate


rc.route_customer_id as route_customer_id
rc.route_id as route_id
rc.customer_id as




SELECT rc.route_customer_id,rc.route_id,rc.customer_id,rc.latitude,rc.longitude,c.customer_name,c.customer_code,c.address,calls.call_id,calls.status,calls.payment_received,calls.order_received,calls.remarks,calls.next_visit_date,calls.sync_status,(select count(id) from call_comment where call_comment.customer_id = calls.customer_id) as count_comment FROM route_customers rc JOIN customers c  ON rc.customer_id = c.store_id LEFT JOIN calls ON rc.route_customer_id = calls.route_customer_id AND rc.customer_id = calls.customer_id


